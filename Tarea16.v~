//: version "1.8.7"

module HA(C, S, B, A);
//: interface  /sz:(78, 65) /bd:[ Li0>B(127/230) Li1>A(49/230) To0<C(143/263) Ro0<S(91/230) ]
input B;    //: /sn:0 {0}(122,340)(208,340){1}
//: {2}(212,340)(257,340)(257,326)(265,326){3}
//: {4}(210,342)(210,371)(265,371){5}
input A;    //: /sn:0 {0}(122,321)(225,321){1}
//: {2}(229,321)(265,321){3}
//: {4}(227,323)(227,366)(265,366){5}
output C;    //: /sn:0 /dp:1 {0}(286,369)(380,369){1}
output S;    //: /sn:0 /dp:1 {0}(286,324)(375,324){1}
//: enddecls

  xor g4 (.I0(A), .I1(B), .Z(S));   //: @(276,324) /sn:0 /delay:" 5" /w:[ 3 3 0 ]
  //: output g3 (C) @(377,369) /sn:0 /w:[ 1 ]
  //: output g2 (S) @(372,324) /sn:0 /w:[ 1 ]
  //: input g1 (B) @(120,340) /sn:0 /w:[ 0 ]
  //: joint g6 (B) @(210, 340) /w:[ 2 -1 1 4 ]
  //: joint g7 (A) @(227, 321) /w:[ 2 -1 1 4 ]
  and g5 (.I0(A), .I1(B), .Z(C));   //: @(276,369) /sn:0 /delay:" 4" /w:[ 5 5 0 ]
  //: input g0 (A) @(120,321) /sn:0 /w:[ 0 ]

endmodule

module RCA2b(A, B);
//: interface  /sz:(40, 40) /bd:[ ]
input [1:0] B;    //: /sn:0 {0}(147,69)(122,69){1}
//: {2}(121,69)(87,69){3}
//: {4}(86,69)(47,69){5}
input [1:0] A;    //: /sn:0 {0}(51,44)(105,44){1}
//: {2}(106,44)(135,44){3}
//: {4}(136,44)(147,44){5}
wire w13;    //: /sn:0 {0}(388,185)(378,185){1}
wire w6;    //: /sn:0 {0}(261,170)(261,160){1}
wire w7;    //: /sn:0 {0}(298,196)(308,196){1}
wire w4;    //: /sn:0 {0}(218,206)(87,206)(87,73){1}
wire w3;    //: /sn:0 {0}(296,89)(306,89){1}
wire w0;    //: /sn:0 {0}(216,99)(122,99)(122,73){1}
wire w12;    //: /sn:0 {0}(388,207)(378,207){1}
wire w10;    //: /sn:0 {0}(428,65)(428,55){1}
wire w1;    //: /sn:0 {0}(216,77)(136,77)(136,48){1}
wire w8;    //: /sn:0 {0}(385,101)(375,101){1}
wire w14;    //: /sn:0 {0}(431,171)(431,161){1}
wire w11;    //: /sn:0 {0}(465,91)(475,91){1}
wire w2;    //: /sn:0 {0}(259,63)(259,53){1}
wire w15;    //: /sn:0 {0}(468,197)(478,197){1}
wire w5;    //: /sn:0 {0}(218,184)(106,184)(106,48){1}
wire w9;    //: /sn:0 {0}(385,79)(375,79){1}
//: enddecls

  tran g8(.Z(w0), .I(B[0]));   //: @(122,67) /sn:0 /R:1 /w:[ 1 2 1 ] /ss:1
  //: input g4 (A) @(49,44) /sn:0 /w:[ 0 ]
  HA g3 (.A(w13), .B(w12), .C(w14), .S(w15));   //: @(389, 172) /sz:(78, 65) /sn:0 /p:[ Li0>0 Li1>0 To0<0 Ro0<0 ]
  HA g2 (.A(w9), .B(w8), .C(w10), .S(w11));   //: @(386, 66) /sz:(78, 65) /sn:0 /p:[ Li0>0 Li1>0 To0<0 Ro0<0 ]
  HA g1 (.A(w5), .B(w4), .C(w6), .S(w7));   //: @(219, 171) /sz:(78, 65) /sn:0 /p:[ Li0>0 Li1>0 To0<0 Ro0<0 ]
  tran g6(.Z(w1), .I(A[0]));   //: @(136,42) /sn:0 /R:1 /w:[ 1 3 4 ] /ss:1
  tran g9(.Z(w4), .I(B[1]));   //: @(87,67) /sn:0 /R:1 /w:[ 1 4 3 ] /ss:1
  tran g7(.Z(w5), .I(A[1]));   //: @(106,42) /sn:0 /R:1 /w:[ 1 1 2 ] /ss:1
  //: input g5 (B) @(45,69) /sn:0 /w:[ 5 ]
  HA g0 (.A(w1), .B(w0), .C(w2), .S(w3));   //: @(217, 64) /sz:(78, 65) /sn:0 /p:[ Li0>0 Li1>0 To0<0 Ro0<0 ]

endmodule

module main;    //: root_module
